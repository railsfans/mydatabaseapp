<h2>Import Products</h2>
<%= form_tag import_repository_index_path, multipart: true do %>
  <%= file_field_tag :file %><br /><h2>number</h2>
  <%= text_field_tag :search, params[:search] %><br />
  <%= submit_tag "Import" %>
<% end %>
<p><%= link_to "Import products", new_boy_import_path %></p>
<h2>list repository</h2><h1>download repository list<%= link_to "Excel", repository_index_path(format: "xls") %></h1>
<table class="admintable" width="100%" >
  <tr>
    <th class="adminth">itemNo</th>
    <th class="adminth">partName</th>
    <th class="adminth">partRef</th>
    <th class="adminth">footprint</th>
    <th class="adminth">manufacturer</th>
    <th class="adminth">quantity</th>
  </tr>
<% for repo in @repositorys %>
<tr>
<td class="admincls0"><%= repo.itemNo %></td>
<td class="admincls0"><%= repo.partName %></td>
<td class="admincls0"><%= repo.partRef %></td>
<td class="admincls0"><%= repo.footprint %></td>
<td class="admincls0"><%= repo.manufacturer %></td>
<td class="admincls0"><%= repo.quantity %></td>
<td class="admincls0"><%= link_to 'Show', repo %></td>
<td class="admincls0"><%= link_to 'Edit', edit_repository_path(repo) %></td>
<td class="admincls0"><%= link_to 'Destroy', repo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
<% end %>
</table>
<%= will_paginate @repositorys %>